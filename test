
CREATE TABLE [CAT]
( 
	[a]                  char(18)  NOT NULL 
)
go

CREATE TABLE [E_1]
( 
	[a]                  char(18)  NOT NULL ,
	[b]                  char(18)  NULL ,
	[c]                  char(18)  NULL 
)
go

ALTER TABLE [CAT]
	ADD CONSTRAINT [XPKCAT] PRIMARY KEY  CLUSTERED ([a] ASC)
go

ALTER TABLE [E_1]
	ADD CONSTRAINT [XPKE_1] PRIMARY KEY  CLUSTERED ([a] ASC)
go
